{
  "name": "northrook/storage",
  "description": "Store arbitrary PHP data to disk, allowing it to be retrieved and modified at runtime, saved and OPcached.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\": "src"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-zend-opcache": "*",
    "symfony/filesystem": "@stable",
    "symfony/var-exporter": "@stable"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan": "@stable",
    "northrook/php-cs": "dev-main"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse"
  }
}
